{
    "name": "ferror/poc-symfony-heroku-travis",
    "description": "",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^2.10|^3.0",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "nelmio/cors-bundle": "^2.1",
        "sentry/sentry-symfony": "^4.1",
        "symfony/console": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/flex": "^1.12",
        "symfony/framework-bundle": "^5.2",
        "symfony/messenger": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.2"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
